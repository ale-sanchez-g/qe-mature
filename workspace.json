{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/express"
  },
  "defaultProject": "mature-api",
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "projects": {
    "mature-api": {
      "root": "apps/mature-api",
      "sourceRoot": "apps/mature-api/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/mature-api",
            "main": "apps/mature-api/src/main.ts",
            "tsConfig": "apps/mature-api/tsconfig.app.json",
            "assets": ["apps/mature-api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/mature-api/src/environments/environment.ts",
                  "with": "apps/mature-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "mature-api:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/mature-api/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/mature-api"],
          "options": {
            "jestConfig": "apps/mature-api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "mature-org": {
      "root": "apps/mature-org",
      "sourceRoot": "apps/mature-org/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/mature-org",
            "index": "apps/mature-org/src/index.html",
            "main": "apps/mature-org/src/main.tsx",
            "polyfills": "apps/mature-org/src/polyfills.ts",
            "tsConfig": "apps/mature-org/tsconfig.app.json",
            "assets": [
              "apps/mature-org/src/favicon.ico",
              "apps/mature-org/src/assets"
            ],
            "styles": ["apps/mature-org/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/mature-org/src/environments/environment.ts",
                  "with": "apps/mature-org/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "mature-org:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "mature-org:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/mature-org/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/mature-org"],
          "options": {
            "jestConfig": "apps/mature-org/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "mature-org-e2e": {
      "root": "apps/mature-org-e2e",
      "sourceRoot": "apps/mature-org-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/mature-org-e2e/cypress.json",
            "tsConfig": "apps/mature-org-e2e/tsconfig.e2e.json",
            "devServerTarget": "mature-org:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "mature-org:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/mature-org-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  }
}
